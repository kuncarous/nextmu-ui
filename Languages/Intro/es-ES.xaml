<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <sys:String x:Key="UpdateSystem.Initializing">Inicializando...</sys:String>
  <sys:String x:Key="UpdateSystem.WaitingServers">Recibiendo servidores de actualización...</sys:String>
  <sys:String x:Key="UpdateSystem.WaitingFilesList">Recibiendo lista de actualización...</sys:String>
  <sys:String x:Key="UpdateSystem.VerifyingFiles">Verificando archivos locales... ([bind]Update.VerifiedCount[/bind]/[bind]Update.VerifyCount[/bind])</sys:String>
  <sys:String x:Key="UpdateSystem.UpdatingFiles">Descargando... ([bind format='{0:F2}']Update.DownloadedSize[/bind]/[bind format='{0:F2}']Update.DownloadSize[/bind][bind]Update.DownloadSizeType[/bind])</sys:String>
