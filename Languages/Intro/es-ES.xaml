  <sys:String x:Key="Popup.Connecting">Conectando...</sys:String>
  <sys:String x:Key="Popup.Buttons.Ok">Ok</sys:String>
  <sys:String x:Key="Popup.Buttons.Yes">Si</sys:String>
  <sys:String x:Key="Popup.Buttons.No">No</sys:String>
  <sys:String x:Key="Popup.Buttons.Accept">Aceptar</sys:String>
  <sys:String x:Key="Popup.Buttons.Cancel">Cancelar</sys:String>
  <sys:String x:Key="Popup.Buttons.Retry">Reintentar</sys:String>
  <sys:String x:Key="Popup.Buttons.Abort">Cancelar</sys:String>
</ResourceDictionary>